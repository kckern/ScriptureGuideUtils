{
  "name": "scripture-guide",
  "version": "1.0.64",
  "description": "A utility for biblical and scripture reference parsing that enables structured queries based on strings of traditional scripture references (e.g. John 3:16).  ",
  "main": "scriptures-compiled.cjs",
  "module": "scriptures-esm.js",
  "types": "scriptures-compiled.d.ts",
  "type": "module",
  "exports": {
    ".": {
      "require": "./scriptures-compiled.cjs",
      "import": "./scriptures-esm.js",
      "types": "./scriptures-compiled.d.ts"
    }
  },
  "scripts": {
    "start": "node test.js",
    "test": "node test.js",
    "build": "babel scriptures.js --out-file scriptures-compiled.cjs && node scripts/convert-to-esm.cjs"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://scripture.guide",
  "repository": {
    "type": "git",
    "url": "git://github.com/kckern/ScriptureGuideUtils.git"
  },
  "keywords": [
    "bible",
    "old testament",
    "new testament",
    "scripture",
    "reference",
    "parsing",
    "query",
    "search",
    "structured",
    "string",
    "lds",
    "book of mormon",
    "doctrine and covenants",
    "pearl of great price"
  ],
  "files": [
    "scriptures-compiled.cjs",
    "scriptures-esm.js",
    "scriptures-compiled.d.ts",
    "scriptures.d.ts",
    "data/"
  ],
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2"
  }
}
