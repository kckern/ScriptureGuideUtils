    "pre_rules":[

        ["[.]{2,}", ";"],
        ["(\\d+) (\\d+)", "$1.$2"],
        ["[.]", ":"],
        ["([a-z])[ .-]*0+", "$1"],
        ["^(\\d+)-", "$1"],
        [" "," "],

        ["(first|1st|\\bi\\b)", "1"],
        ["(second|2nd|\\bii\\b)", "2"],
        ["(third|3rd|\\biii\\b)", "3"],
        ["(fourth|4th|\\biv\\b)", "4"],
        ["([0-9])th\\b", "$1"],
        ["([1-4])(st|nd|d|rd|th)\\b", "$1"],
        [",*\\s*\\b(?:ch\\b|chap(?:t*er)*s*)\\.*\\,*\\s*(\\d+)", "$1"],
        [",*\\s*\\b(?:sec\\b|section)\\.*\\s*(\\d+)", "$1"],
        ["([0-9]),*\\s*\\b(?:chapt*ers*|ch|chptrs*)\\.*,*\\s*([0-9])", "$1:$2"],
        ["([a-z]),*\\s*\\b(?:chapt*ers*|ch|chptrs*)\\s*([0-9])", "$1 $2"],
        ["[()]+", " "],  
        ["[\\s.]*[–—−\\-]+[\\s.]*", "-"], 
        ["([;,])\\s*and\\s*", "$1"],
        ["([0-9])[ .]*and[ .]*", "$1,"],
        ["(\\d)(\\.|: )(\\d)", "$1:$3"],
        ["\\s+(\\d+)[-–—](\\d+:\\d+)", " $1:1-$2"],
        ["(\\d)([A-Z])", "$1 $2"],
        ["(\\d) ([A-Z])$", "$1$2"],
        ["([0-9]),\\s*([1-4]*\\s*[a-z])", "$1; $2"],
        ["([a-z]), ([0-9])", "$1 $2"],  
        ["\\.", " "],  
        [" - ", " "],  
        ["; ", ";"],  
        [": ", ":"],  
        ["(\\D)-(\\d)", "$1 $2"],  
        ["([a-z]),", "$1"],  
        ["jst(,| of )", "JST "],  
        ["jst([a-z])", "JST $1"],  
        [",\\s*((?:\\d+\\s)*[a-z])", "; $1"],
        ["\\b\\s*cf\\.*\\s*\\b", "; "],
        ["\\bv*ve*r*s*e*s*\\s*([0-9])", ":$1"],
        ["(, *)*\\bverses*\\s*([0-9])", ":$1"],
        ["\\s*:", ":"],  
        [";\\s*:", ":"],
        ["(\\d+) *to *(\\d+)", "$1-$2"], 
        ["([—-])h", "\u2014H"],
        ["([—-])m", "\u2014M"],
        ["([A-za-z])(\\d)", "$1 $2"],
        ["([0-9]),\\s*([1-4]*\\s*[a-z])", "$1; $2"],
        ["o([nf])", "o$1"],
        ["([;,])", "$1 "],
        ["(the )*book of", ""]
    ],

    "post_rules":[
        ["(Words of Mormon|Articles of Faith|4 Nephi|Jarom|Enos|Omni|Obadiah|2 John|3 John|Philemon|Jude|First Vision|Joseph Smith—.+)\\s+([2-9]|(\\d\\d+))","$1 1:$2"],
        ["(^|;\\s*)(Nephi|cor|kings|chron|thes|pet|tim)","$1 1 $2"],
        ["\\s+"," "],
        ["([a-z]\\s*\\d+)(-\\d+:\\d+)","$1:1$2"],
        ["--","\u2014"]
    ]
}